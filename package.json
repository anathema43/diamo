{
  "name": "ramro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "cy:run:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "cy:run:desktop": "cypress run --config viewportWidth=1280,viewportHeight=720",
    "cy:run:headless": "cypress run --headless",
    "cy:run:all": "cypress run --headless --browser chrome",
    "cy:run:complete": "npm run cy:run:all && npm run test",
    "cy:run:ci": "cypress run --headless --browser chrome --reporter junit --reporter-options mochaFile=cypress/results/test-results-[hash].xml",
    "test:all": "npm run test && npm run cy:run:all",
    "cy:run:script": "node cypress/scripts/run-all-tests.js",
    "cy:run:parallel": "node cypress/scripts/parallel-runner.js",
    "cy:run:static": "cypress run --headless --spec 'cypress/e2e/10-static-pages.cy.js,cypress/e2e/11-footer-pages.cy.js'",
    "cy:run:security": "cypress run --headless --spec 'cypress/e2e/13-security-testing.cy.js'",
    "cy:run:realtime": "cypress run --headless --spec 'cypress/e2e/12-real-time-features.cy.js'",
    "cy:run:architecture": "cypress run --headless --spec 'cypress/e2e/15-architectural-integrity.cy.js'",
    "cy:run:images": "cypress run --headless --spec 'cypress/e2e/16-image-optimization.cy.js'",
    "cy:run:artisans": "cypress run --headless --spec 'cypress/e2e/17-artisan-cultural-content.cy.js'",
    "cy:run:critical": "cypress run --headless --spec 'cypress/e2e/01-authentication.cy.js,cypress/e2e/13-security-testing.cy.js,cypress/e2e/12-real-time-features.cy.js'",
    "cy:run:cultural": "cypress run --headless --spec 'cypress/e2e/17-artisan-cultural-content.cy.js'",
    "cy:run:foundational": "cypress run --headless --spec 'cypress/e2e/auth.cy.js,cypress/e2e/shopping_cart.cy.js,cypress/e2e/order_history.cy.js'",
    "cy:run:admin": "cypress run --headless --spec 'cypress/e2e/admin-file-upload.cy.js'",
    "cy:run:core": "cypress run --headless --spec 'cypress/e2e/01-authentication.cy.js,cypress/e2e/02-shopping-cart.cy.js,cypress/e2e/03-checkout.cy.js'",
    "cy:run:user-features": "cypress run --headless --spec 'cypress/e2e/01-authentication.cy.js,cypress/e2e/02-shopping-cart.cy.js'",
    "cy:run:new-features": "cypress run --headless --spec 'cypress/e2e/product-reviews.cy.js,cypress/e2e/wishlist-functionality.cy.js'",
    "test:security": "vitest run --reporter=verbose src/**/*.security.test.js",
    "test:integration": "vitest run --reporter=verbose src/**/*.integration.test.js",
    "test:all-comprehensive": "npm run test && npm run test:security && npm run cy:run:all",
    "test:stores": "vitest run src/store/__tests__/*.test.js",
    "test:components": "vitest run src/components/__tests__/*.test.jsx",
    "test:utils": "vitest run src/utils/__tests__/*.test.js",
    "test:foundational": "npm run test:utils && npm run test:stores && npm run cy:run:foundational && npm run cy:run:new-features",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "npm run build && firebase deploy",
    "analyze-bundle": "npm run build && npx vite-bundle-analyzer dist",
    "optimize": "npm run build && npm run analyze-bundle",
    "cost-report": "node scripts/generateCostReport.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "algoliasearch": "^4.24.0",
    "firebase": "^12.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.19",
    "@cypress/code-coverage": "^3.12.0",
    "cypress": "^14.5.2",
    "cypress-axe": "^1.5.0",
    "@testing-library/cypress": "^10.0.2",
    "eslint-plugin-cypress": "^3.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^7.0.4",
    "vitest": "^1.0.4"
  }
}
